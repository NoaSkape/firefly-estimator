{
  "name": "firefly-estimator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "concurrently \"vite\" \"vercel dev --listen 3000\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "node backend/server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\"",
    "dev:api": "node backend/dev-server.js",
    "dev:local": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "migrate": "node -r dotenv/config scripts/migrate-data.js",
    "migrate:slugs": "node -r dotenv/config scripts/add-slugs.js"
  },
  "dependencies": {
    "@clerk/backend": "^2.6.3",
    "@clerk/clerk-sdk-node": "^5.0.32",
    "@clerk/clerk-react": "^5.39.0",
    "@cloudinary/react": "^1.14.3",
    "@cloudinary/url-gen": "^1.21.0",
    "axios": "^1.7.2",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "mongodb": "^6.18.0",
    "papaparse": "^5.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.51.0",
    "react-router-dom": "^7.7.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite": "^7.0.4"
  }
}
