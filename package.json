{
  "name": "firefly-estimator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "seed:models": "node -r dotenv/config scripts/seed-models.cjs",
    "seed:complete": "node scripts/seed-models-complete.js",
    "dev": "concurrently \"vite\" \"npm run dev:api\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "node backend/server.cjs",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\"",
    "dev:api": "node backend/dev-server.js",
    "dev:local": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "migrate": "node -r dotenv/config scripts/migrate-data.js",
    "migrate:slugs": "node -r dotenv/config scripts/add-slugs.js",
    "debug:dev": "cross-env DEBUG_ADMIN=true VITE_DEBUG_ADMIN=true concurrently \"vite\" \"npm run dev:api\"",
    "debug:api": "cross-env DEBUG_ADMIN=true node backend/dev-server.js",
    "debug:server": "cross-env DEBUG_ADMIN=true node backend/dev-server.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clerk/backend": "^2.6.3",
    "@clerk/clerk-react": "^5.42.0",
    "@clerk/themes": "^2.4.9",
    "@cloudinary/react": "^1.14.3",
    "@cloudinary/url-gen": "^1.21.0",
    "@heroicons/react": "^2.2.0",
    "@sparticuz/chromium": "^138.0.2",
    "@stripe/react-stripe-js": "^3.9.2",
    "@stripe/stripe-js": "^7.8.0",
    "chart.js": "^4.5.0",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.1",
    "isomorphic-dompurify": "^2.26.0",
    "jspdf": "^3.0.1",
    "mongodb": "^6.18.0",
    "papaparse": "^5.4.1",
    "pdf-lib": "^1.17.1",
    "puppeteer": "^24.19.0",
    "puppeteer-core": "^24.19.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.51.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.7.1",
    "serverless-http": "^3.2.0",
    "socket.io-client": "^4.8.1",
    "stripe": "^16.12.0",
    "undici": "^7.15.0",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "mongodb-memory-server": "^10.0.0",
    "postcss": "^8.4.35",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.1",
    "vite": "^7.0.4",
    "vitest": "^2.1.8"
  }
}
